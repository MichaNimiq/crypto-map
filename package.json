{
  "name": "nimiq-map",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "api:generate": "./node_modules/@openapitools/openapi-generator-cli/main.js generate -g typescript-fetch -i https://raw.githubusercontent.com/nimiq/merchant-map-backend/master/docs/openapi.yaml -o ./src --additional-properties=typescriptThreePlus=true,legacyDiscriminatorBehavior=false,modelPropertyNaming='original',enumPropertyNaming='original' -o ./src/api/ && yarn build"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.2",
    "pinia": "^2.0.16",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.3",
    "vue3-google-map": "^0.15.0"
  },
  "devDependencies": {
    "@googlemaps/markerclusterer": "^2.0.10",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^16.11.45",
    "@types/qs": "^6.9.7",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^9.1.0",
    "autoprefixer": "^10.4.11",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.0.0",
    "focus-visible": "^5.2.0",
    "google-maps": "^4.3.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "prettier": "^2.5.1",
    "sass": "^1.54.4",
    "tailwindcss": "^3.1.8",
    "tailwindcss-nimiq-theme": "^0.0.5",
    "typescript": "~4.7.4",
    "vite": "^3.0.1",
    "vite-plugin-rewrite-all": "^1.0.0",
    "vue-tsc": "^0.38.8"
  }
}
